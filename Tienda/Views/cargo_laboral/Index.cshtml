        <td>
            @Html.DisplayFor(modelItem => item.CARGO_LABORAL1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESCRIPCION_CARGO_LABORAL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SALARIO_MENSUAL_CARGO_LABORAL)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_CARGO_LABORAL }) |
