</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CARGO_LABORAL1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESCRIPCION_CARGO_LABORAL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SALARIO_MENSUAL_CARGO_LABORAL)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
